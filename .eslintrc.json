{
    "env": {
        "browser": true,
        "commonjs": true,
        "es2021": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parserOptions": {
        "ecmaVersion": "latest"
    },
    "rules": {
        "no-plusplus": "off",
        "prefer-destructuring": "off",
        "max-len": "off",
        "radix":"off",
        "no-loop-func":"off"
    }
}
